{
    "workbench.tree.indent": 20,
    "editor.formatOnPaste": true,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.fontSize": 15,
    "editor.lineHeight": 28,
    "editor.fontSize": 18,
    "editor.fontFamily": "Fira Code Retina",
    "editor.fontLigatures": true,
    // "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "vim.leader": "<space>",
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "q",
                "q",
            ],
            "after": [
                "$"
            ]
        }
    ],
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-b>": false,
        "<C-f>": false
    },
    "vim.foldfix": true,
    "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
    "php-cs-fixer.executablePathWindows": "", //eg: php-cs-fixer.bat
    "php-cs-fixer.onsave": true,
    "php-cs-fixer.rules": "@PSR2",
    "php-cs-fixer.config": ".php_cs;.php_cs.dist",
    "php-cs-fixer.allowRisky": false,
    "php-cs-fixer.pathMode": "override",
    "php-cs-fixer.exclude": [],
    "php-cs-fixer.autoFixByBracket": true,
    "php-cs-fixer.autoFixBySemicolon": false,
    "php-cs-fixer.formatHtml": false,
    "php-cs-fixer.documentFormattingProvider": true,
    "blade.format.enable": true,
    "files.watcherExclude": {
        "**/vendor/**": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "statusBar.background": "#1A1A1A",
        "statusBar.noFolderBackground": "#212121",
        "statusBar.debuggingBackground": "#263238",
        "panel.background": "#1d1d1d",
        "editorCursor.foreground": "#ccc",
        "minimap.background": "#22222233"
    },
    "workbench.editor.enablePreview": false,
    "vetur.format.options.tabSize": 4,
    "php-cs-fixer.lastDownload": 1601442844670,
    "vim.useSystemClipboard": true,
    "editor.suggestSelection": "first",
    "beautify.language": {
        "js": {
            "type": [
                "javascript",
                "json"
            ],
            "filename": [
                ".jshintrc",
                ".jsbeautifyrc"
            ]
            // "ext": ["js", "json"]
            // ^^ to set extensions to be beautified using the javascript beautifier
        },
        "css": [
            "css",
            "scss",
            "less"
        ],
        "html": [
            "htm",
            "html",
            "django-html"
        ]
        // ^^ providing just an array sets the VS Code file type
    },
    "emmet.includeLanguages": {
        "django-html": "html",
        "javascript": "javascriptreact",
        "twig": "html",
    },
    "python.formatting.provider": "autopep8",
    "python.linting.pylintArgs": [
        "--load-plugins=pylint_django"
    ],
    "editor.minimap.renderCharacters": false,
    "editor.minimap.maxColumn": 60,
    "window.zoomLevel": 0,
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[php]": {
        "editor.defaultFormatter": "junstyle.php-cs-fixer"
    },
    "[django-html]": {
        "editor.defaultFormatter": "HookyQR.beautify",
        "editor.quickSuggestions": {
            "other": true,
            "comments": true,
            "strings": true
        }
    },
    "prettier.jsxBracketSameLine": false,
    "html.format.wrapAttributes": "preserve",
    "python.autoComplete.addBrackets": true,
    "python.languageServer": "Pylance",
    "[css]": {
        "editor.defaultFormatter": "MikeBovenlander.formate"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    // no annoying start up page
    "workbench.startupEditor": "none",
    // "editor.renderControlCharacters": false,
    "explorer.openEditors.visible": 0,
    // don't copy if you accidentally hit ctrl-c
    "editor.emptySelectionClipboard": false,
    "editor.quickSuggestionsDelay": 0,
    "diffEditor.ignoreTrimWhitespace": false,
    "zenMode.fullScreen": false,
    "[less]": {
        "editor.defaultFormatter": "MikeBovenlander.formate"
    },
    "python.showStartPage": false,
    "editor.lineNumbers": "relative",
    "[scss]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "indentRainbow.errorColor": "rgba(40,0,0,0.04)",
    "indentRainbow.colors": [
        "rgba(136,192,208,0.06)",
        "rgba(235,203,139,0.06)",
        "rgba(180,142,173,0.06)",
        "rgba(208,135,112,0.06)"
    ],
    "workbench.iconTheme": "vscode-icons",
    "hediet.vscode-drawio.local-storage": "",
    "workbench.colorTheme": "Community Material Theme Darker High Contrast",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "python.analysis.completeFunctionParens": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },

}